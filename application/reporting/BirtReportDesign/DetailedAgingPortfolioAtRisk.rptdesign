            <method name="open"><![CDATA[Packages.java.util.logging.Logger.getLogger("").info("DFLAIAB");

var branchId = reportContext.getParameterValue("branchId");

var loanOfficerId = reportContext.getParameterValue("loanOfficerId");

var loanProductId = reportContext.getParameterValue("loanProductId");

current = reportsDataService.getTotalOutstandingPrincipalOfLoanAccountsInActiveGoodStanding(branchId,loanOfficerId,loanProductId);
loans = reportsDataService.getLoanAccountsInActiveBadStanding(branchId,loanOfficerId,loanProductId);
iterator = loans.iterator();]]></method>
            <method name="fetch"><![CDATA[if(current == null)
{
    row["Current"] = 0;
}else{
    row["Current"] = current;
}

if(iterator.hasNext() == false ){
	return false;
}
Packages.java.util.logging.Logger.getLogger("").info("FETCH");
var loan = iterator.next( );
row["LoanNumber"]= loan.getGlobalAccountNum();
row["Client"] = loan.getCustomer().getDisplayName();
row["LoanAmount"] = loan.getLoanAmount().getAmount();
row["Balance"] = loan.getRemainingPrincipalAmount().getAmount();
row["Principal"] = loan.getTotalPrincipalAmountInArrearsAndOutsideLateness().getAmount();
row["Interest"] = loan.getTotalInterestAmountInArrearsAndOutsideLateness().getAmount();
row["DaysInArrears"] = loan.getDaysInArrears();

if (loan.getDaysInArrears()>=1 &amp;&amp; loan.getDaysInArrears() <=30)
row["1-30"] = row["Balance"];

if (loan.getDaysInArrears()>=31 &amp;&amp; loan.getDaysInArrears() <=60)
row["31-60"] = row["Balance"];

if (loan.getDaysInArrears()>=61 &amp;&amp; loan.getDaysInArrears() <=90)
row["61-90"] = row["Balance"];

if (loan.getDaysInArrears()>=91 &amp;&amp; loan.getDaysInArrears()<=180)
row["91-180"] =row["Balance"];

if (loan.getDaysInArrears()>=181)
row[">180"] =row["Balance"];


return true;]]></method>
            <method name="close"><![CDATA[loans = null;
iterator = null;
current = null;
Packages.java.util.logging.Logger.getLogger("").info("DFLAIAB-CLOSE");]]></method>
