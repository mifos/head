<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping > 
  <class name="org.mifos.reports.business.ReportsParamsValue" table="report_parameter" >
     <id name="parameterId" column="parameter_id" type="int">
         <generator class="native" />
     </id>
    	<property name="name" column="name" type="string" />
    	<property name="classname" column="classname" type="string" />
		<property name="data" column="data" type="string" />
		<property name="description" column="description" type="string" />
		<property name="type" column="type" type="string" />
		<property name="datasourceId" column="datasource_id" type="int" />
      
	</class> 
     
</hibernate-mapping> 
