"novamente."

#: errors.InvalidReceiptDate
msgid "<li>Please specify a valid receipt date.</li>"
msgstr "<li>Por favor, especifique uma data de recepção válida.</li>"

#: errors.invalidamount
msgctxt "errors.invalidamount"
msgid ""
"<li>The amount specified for {0} of customer {1} is invalid. Please specify "
"a valid amount.</li>"
msgstr ""
"<li>O valor especificado para {0} do cliente {1} é inválido. Por favor "
"especifique um valor válido.</li>"

#: errors.invalidprepayamount
msgid ""
"<li>Product {0} for customer {1} has two or more accounts open. Prepay these "
"accounts individually.</li>"
msgstr ""
"<li>Prooducto {0} para cliente {1} tem dois o mais contas abertas. Prepague "
"estas contas individualmente.</li>"

#: errors.invalidaccollections
msgid ""
"<li>The amount specified for {0} of customer {1} must be exact amount due or "
"0. Please specify a valid amount.</li>"
msgstr ""
"<li>O valor especificado para {0} do cliente {1} é inválido. Por favor "
"especifique um valor válido.</li>"

#: errors.invalidamountvalue
msgctxt "errors.invalidamountvalue"
msgid ""
"<li>The amount specified for {0} of customer {1} is invalid. Please specify "
"a valid amount.</li>"
msgstr ""
"<li>O valor especificado para {0} do cliente {1} é inválido. Por favor "
"especifique um valor válido.</li>"

#: errors.invaliddate
msgid ""
"<li>Please specify a valid date-- {0} must be between the last meeting date "
"and today.</li>"
msgstr ""
"<li>Por favor especifique uma data válida-- {0} deve situar-se entre a data "
"do último encontro e a data de hoje.</li>"

#: errors.mandatoryenter
msgid "<li>Please specify {0}.</li>"
msgstr "<li>Por Favor {0}.</li>"

#: errors.mandatoryselect
msgid "<li>Please select the {0}.</li>"
msgstr "<li>Por favor selecione o/a {0}.</li>"

#: errors.meetingdate
msgid "<li>Please specify a valid date-- {0} can only be today's date.</li>"
msgstr ""
"<li>Por favor especifique uma data válida-- {0} só pode ser a data de "
"hoje.</li>"

#: errors.update
msgid ""
"The following accounts have not been updated due to simultaneous "
"updates/insufficient balance/invalid disbursement date/invalid repayment: "
"{0}"
