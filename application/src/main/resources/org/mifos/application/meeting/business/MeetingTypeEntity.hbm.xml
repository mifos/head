<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.meeting.business.MeetingTypeEntity" table="meeting_type">
        <id name="meetingTypeId" column="meeting_type_id" type="short" access="field">
            <generator class="native"/>
        </id>
         <property name="meetingPurpose" column="meeting_purpose" type="string" access="field" />
         <property name="description" column="description" type="string" access="field"/> 
    </class>
</hibernate-mapping>