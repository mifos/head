<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.customers.office.business.OfficeStatusEntity" table="OFFICE_STATUS">
        <id name="id" column="STATUS_ID" type="short">
            <generator class="native"/>
        </id>
			<many-to-one name="lookUpValue" column="LOOKUP_ID" class="org.mifos.application.master.business.LookUpValueEntity" 
    			cascade="none" update="false" unique="true"/>	
		
    </class>

</hibernate-mapping>