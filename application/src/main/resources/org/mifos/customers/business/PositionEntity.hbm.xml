<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>
	<class name="org.mifos.customers.business.PositionEntity"
		table="position">
		<id name="id" column="position_id" unsaved-value="-1">
			<generator class="native" />
		</id>

        <many-to-one name="lookUpValue" column="lookup_id" class="org.mifos.application.master.business.LookUpValueEntity" 
    			cascade="none" update="false" unique="true"/>
	</class>
</hibernate-mapping>