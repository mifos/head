<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.mifos.accounts.financial.business.COAIDMapperEntity" table="COA_IDMAPPER">
		<id name="constantId" column="CONSTANT_ID" access="field">
			<generator class="native" />
		</id>
		<many-to-one name="coa" column="COA_ID" class="org.mifos.accounts.financial.business.COABO" outer-join="true" cascade="none" update="false" insert="false" unique="true" access="field" />


	</class>
	<query name="GETALLCOA">
		<![CDATA[
		    from  org.mifos.accounts.financial.business.COAIDMapperEntity coaIdMapper
		]]>
	</query>

</hibernate-mapping>
