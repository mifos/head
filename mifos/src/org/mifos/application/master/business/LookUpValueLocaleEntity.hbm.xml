<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping > 
  <class name="org.mifos.application.master.business.LookUpValueLocaleEntity" table="LOOKUP_VALUE_LOCALE" >
  	<id column="LOOKUP_VALUE_ID" name="lookUpValueId" type="java.lang.Integer" > 
		<generator class="native"></generator>
    </id>
    <property column="LOOKUP_ID"  name="lookUpId"  />
    <property column="LOOKUP_VALUE"  name="lookUpValue"  type="java.lang.String"/>
    <property column="LOCALE_ID"  name="localeId" />
    
    <many-to-one name="locale" column="LOCALE_ID" class="org.mifos.application.master.business.SupportedLocalesEntity"  unique="true" lazy="true" insert="false" update="false"/>    
    
  
  </class> 
</hibernate-mapping> 