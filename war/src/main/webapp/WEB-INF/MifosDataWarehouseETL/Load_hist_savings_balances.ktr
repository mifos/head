    <replace_variables>Y</replace_variables>
    <sql>insert into hist_savings_balances (as_of_date, savings_account_id, currency_id, prd_offering_id, customer_id, group_id, center_id,
loan_officer_id, branch_id, formed_by_loan_officer_id, 
total_deposit_amount, total_interest_amount, total_withdrawal_amount, balance)

select &apos;?&apos; as as_of_date, s.savings_account_id, s.currency_id, p.prd_offering_id, 
cust.customer_id, gr.customer_id as group_id, cen.customer_id as center_id,
lo.personnel_id as loan_officer_id, o.office_id as branch_id, flo.personnel_id as formed_by_loan_officer_id,
ifnull(sum(sav.deposit_amount), 0.0) as total_deposit_amount,
ifnull(sum(sav.interest_amount), 0.0) as total_interest_amount,
ifnull(sum(sav.withdrawal_amount), 0.0) as total_withdrawal_amount,
ifnull(sum(sav.deposit_amount + sav.interest_amount - sav.withdrawal_amount), 0.0) as balance
from dim_savings s
left join dim_product p on p.product_key = s.product_key
left join dim_customer cust on cust.customer_key = s.customer_key
left join dim_customer gr on gr.customer_key = s.group_key
left join dim_customer cen on cen.customer_key = s.center_key
left join dim_personnel lo on lo.personnel_key = s.loan_officer_key
left join dim_office o on o.office_key = s.branch_key
left join dim_personnel flo on flo.personnel_key = s.formed_by_loan_officer_key
left join fact_savings_transactions sav on sav.savings_account_id = s.savings_account_id and sav.action_date &lt;= &apos;?&apos; 
where s.valid_from &lt;= &apos;?&apos; and s.valid_to &gt; &apos;?&apos;    
and s.savings_status in (&apos;SavingsStatus-Active&apos;, &apos;SavingsStatus-Inactive&apos;)
group by as_of_date,s.savings_account_id;</sql>
