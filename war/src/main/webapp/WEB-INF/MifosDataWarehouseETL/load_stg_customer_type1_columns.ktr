  <step>
    <name>Latest Customer Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select c.customer_id, c.display_name, 
c.date_of_birth,
c.government_id,
cd.num_children as num_children, 
bus_lv.lookup_value as business_activity,
gen_lv.lookup_name as gender, 
eth_lv.lookup_value as ethnicity, 
cit_lv.lookup_value as citizenship, 
hand_lv.lookup_value as handicapped, 
mar_lv.lookup_value as marital_status, 
edu_lv.lookup_value as education_level
 
from customer c
left join customer_detail cd on cd.customer_id = c.customer_id
left join lookup_value_locale bus_lv on bus_lv.lookup_id = cd.business_activities 
left join lookup_value gen_lv on gen_lv.lookup_id = cd.gender 
left join lookup_value_locale eth_lv on eth_lv.lookup_id = cd.ethnicity 
left join lookup_value_locale cit_lv on cit_lv.lookup_id = cd.citizenship 
left join lookup_value_locale hand_lv on hand_lv.lookup_id = cd.handicapped 
left join lookup_value_locale mar_lv on mar_lv.lookup_id = cd.marital_status 
left join lookup_value_locale edu_lv on edu_lv.lookup_id = cd.education_level   

where (
			(c.created_date between date(?) and date(?))
		or (c.updated_date between date(?) and date(?))
															)
 </sql>
