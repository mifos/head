  <step>
    <name>Identify Savings Accounts to be Adjusted and From When</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <sql>
select sav.savings_account_id, min(actiondate.date_value) as first_date, date_sub(cutoffdate.date_value, interval 1 day) as last_date
from fact_savings_transactions sav
join dim_date cutoffdate on cutoffdate.date_value = ?

join dim_date actiondate on actiondate.date_key = sav.action_date_key

where sav.action_date_key &lt; cutoffdate.date_key
and sav.created_date_key &gt;= cutoffdate.date_key   
and not (deposit_amount = 0 and sav.withdrawal_amount = 0 and sav.interest_amount = 0)
group by sav.savings_account_id</sql>
    <limit>0</limit>
    <lookup>Get Start Date Variable</lookup>
    <execute_each_row>Y</execute_each_row>
