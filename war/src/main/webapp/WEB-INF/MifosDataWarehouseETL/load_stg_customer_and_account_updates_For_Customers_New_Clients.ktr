    <sql>
select c.created_date as effective_date, c.customer_level_id as level_id, c.customer_id as entity_id, 
&apos;A&apos; as change_type, 1 as change_order, ifnull(cld.old_value, lv.lookup_name) as updated_status, ifnull(c.customer_formedby_id, 0) as customer_formedby_id,

c.parent_customer_id as current_parent_customer_id,
ch.parent_id as first_hierarchy_entry,
cldGF.new_value as first_groupFlag_new_value

from customer c
join customer_state cs on cs.status_id = c.status_id
join lookup_value lv on lv.lookup_id = cs.status_lookup_id


&#47;* find first status if there has been a status change*&#47;
left join change_log cl on c.customer_id = cl.entity_id
                        and cl.entity_type = 1
                        and cl.change_log_id = 
                            (select min(clmin.change_log_id)
                            from change_log clmin
                            join change_log_detail cldmin on cldmin.change_log_id = clmin.change_log_id
                                                and cldmin.field_name =  &apos;Status&apos;
                            where clmin.entity_id = c.customer_id
                            and clmin.entity_type = 1)
left join change_log_detail cld on cld.change_log_id = cl.change_log_id
                        and cld.field_name =  &apos;Status&apos;
                        

&#47;*get parent from the hierarchy table*&#47;
left join customer_hierarchy ch on ch.customer_id = c.customer_id
                and ch.hierarchy_id = 
                    (select min(hierarchy_id) from customer_hierarchy chmin
                    where chmin.customer_id = c.customer_id)


&#47;* check for first groupFlag change*&#47;
left join change_log clGF on c.customer_id = clGF.entity_id
                        and clGF.entity_type = 1
                        and clGF.change_log_id = 
                            (select min(clminGF.change_log_id)
                            from change_log clminGF
                            join change_log_detail cldminGF on cldminGF.change_log_id = clminGF.change_log_id
                                                and cldminGF.field_name =  &apos;groupFlag&apos;
                            where clminGF.entity_id = c.customer_id
                            and clminGF.entity_type = 1)
left join change_log_detail cldGF on cldGF.change_log_id = clGF.change_log_id
                        and cldGF.field_name =  &apos;groupFlag&apos;


where c.customer_level_id = 1 
and c.created_date between date(?) and date(?)

</sql>
