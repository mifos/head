        <jsScript_script>function getParentId()
{
	if (first_hierarchy_entry == null)
	{
		return current_parent_customer_id;
	}
	return first_hierarchy_entry;
}


var parent_id = null;

if (!(first_groupFlag_new_value == null || first_groupFlag_new_value == &quot;-&quot; || first_groupFlag_new_value == &quot;0&quot; || first_groupFlag_new_value == &quot;1&quot;))
{
	writeToLog(&quot;e&quot;, &quot;Invalid first_groupFlag_new_value: &quot; + first_groupFlag_new_value);
	ERROR_TRANSFORMATION;
}
else
{
	if (first_groupFlag_new_value == null || first_groupFlag_new_value == &quot;-&quot;) &#47;&#47;no change in group membership
