			parent_id = getParentId();
		}
	}
	else
	{

		if (first_groupFlag_new_value == &quot;0&quot;) &#47;&#47;had group membership removed so must have had it at create
		{
			parent_id = getParentId();
		}
	}
}</jsScript_script>
