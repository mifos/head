  <step>
    <name>Read account_trxn for Savings Transactions</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select atxn.action_date, cast(ifnull(atxn.due_date, &apos;3001-01-01&apos;) as date) as due_date,
date(atxn.created_date) created_date, atxn.account_action_id, 
atxn.account_trxn_id, atxn.account_id as savings_account_id, atxn.payment_id, atxn.installment_id, ap.payment_type_id,
stxn.deposit_amount, stxn.withdrawal_amount, stxn.interest_amount
from account_trxn atxn
inner join  savings_trxn_detail stxn on atxn.account_trxn_id = stxn.account_trxn_id
inner join account_payment ap on ap.payment_id = atxn.payment_id
where atxn.account_action_id in (6,7, 11, 14)

and atxn.created_date &gt;= date(?) 
and atxn.created_date &lt; date(?)

&#47;* 
6 Deposit
7 Withdrawal
11 Interest_posting
14 SavingsAdjustment - but covers deposits and withdrawals
*&#47;</sql>
    <limit>0</limit>
