  <step>
    <name>Extract Product Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select po.created_date, po.prd_offering_id, po.prd_offering_name, po.prd_offering_short_name, 
lvprdtype.lookup_name as product_type, 
ifnull(lvsavingstype.lookup_name,&apos;n&#47;a&apos;) as savings_type, 
ifnull(lvra.lookup_name,&apos;n&#47;a&apos;) as per_individual_or_complete_group
from prd_offering po
join prd_type pt on pt.prd_type_id = po.prd_type_id
join lookup_value lvprdtype on lvprdtype.lookup_id = pt.prd_type_lookup_id
left join savings_offering so on so.prd_offering_id = po.prd_offering_id
left join savings_type st on st.savings_type_id = so.savings_type_id
left join lookup_value lvsavingstype on lvsavingstype.lookup_id = st.lookup_id
left join recommended_amnt_unit ra on ra.recommended_amnt_unit_id = so.recommended_amnt_unit_id
left join lookup_value lvra on lvra.lookup_id = ra.lookup_id</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
