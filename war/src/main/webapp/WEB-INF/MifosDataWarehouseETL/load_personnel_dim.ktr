  <step>
    <name>Extract Personnel Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>
select p.*
from
    (
    &#47;* first offices *&#47;
    (select -1 as the_order, p.personnel_id, &apos;n&#47;a&apos; as display_name, false as is_loan_officer,
        if(cld.sequence_num is null, o.office_id, null) as office_id,
        if(cld.sequence_num is null, o.display_name, cld.old_value) office_name, 
        &#47;*extra ifnull to ensure mifos personnel entry gets transferred*&#47;
        ifnull(if(cld.sequence_num is null, pd.date_of_joining_branch, p.created_date), date(&apos;1900-01-01&apos;)) as date_of_joining_branch,
        if(cld.sequence_num is null, null, cl.changed_date) as date_of_leaving_branch
        from personnel p
        join personnel_details pd on pd.personnel_id = p.personnel_id
        join office o on o.office_id = p.office_id
        &#47;* find first office move if there has been an office move *&#47;
        left join change_log cl on p.personnel_id = cl.entity_id
                        and cl.entity_type = 17
                        and cl.change_log_id = 
                            (select min(clmin.change_log_id)
                            from change_log clmin
                            join change_log_detail cldmin on cldmin.change_log_id = clmin.change_log_id
                                                and cldmin.field_name =  &apos;Office&apos;
                            where clmin.entity_id = p.personnel_id
                            and clmin.entity_type = 17)
        left join change_log_detail cld on cld.change_log_id = cl.change_log_id
                        and cld.field_name =  &apos;Office&apos;)

    union all
    
        (select cl.change_log_id as the_order, p.personnel_id, &apos;n&#47;a&apos; as display_name, false as is_loan_officer,
			null as office_id, cld.new_value as office_name, 
			cl.changed_date as date_of_joining_branch,
        
            (select clnext.changed_date &#47;* find next office move if there has been one and use that date*&#47;
            from change_log clnext 
            join change_log_detail cldnext on cldnext.change_log_id = clnext.change_log_id
                                        and cldnext.field_name =  &apos;Office&apos;
            where p.personnel_id = clnext.entity_id
            and clnext.change_log_id &gt; cl.change_log_id
            and clnext.entity_type = 17
            order by cl.change_log_id 
            limit 1) as date_of_leaving_branch
            
        from personnel p
        join change_log cl on cl.entity_id = p.personnel_id and cl.entity_type = 17
        join change_log_detail cld on cld.change_log_id = cl.change_log_id
                    and cld.field_name = (&quot;office&quot;))) p
                                      
order by 2,1;
</sql>
