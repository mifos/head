        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here


var date_leave_branch = &apos;2199-12-31&apos;;
if (!(date_of_leaving_branch == null))
{
	date_leave_branch = date_of_leaving_branch;
}


var DBConnection = &quot;DestinationDB&quot;;
var sql = &quot;select count(*) from dim_personnel p&quot;;
sql = sql + &quot; join dim_office o on o.office_key = p.office_key&quot;;
sql = sql + &quot; where p.personnel_id = &quot; + personnel_id;
sql = sql + &quot; and o.office_id = &quot; + office_id;
sql = sql + &quot; and p.valid_from =  date(&apos;&quot; +  date_of_joining_branch + &quot;&apos;)&quot;;
sql = sql + &quot; and ifnull(p.valid_to, &apos;2199-12-31&apos;) =  date(&apos;&quot; +  date_leave_branch + &quot;&apos;)&quot;;
var sqlResults = fireToDB(DBConnection, sql);


var queryCount = parseInt(sqlResults[0][0]);

if (queryCount &gt; 0)
{
	trans_Status = SKIP_TRANSFORMATION;
} 
else
{
	trans_Status = CONTINUE_TRANSFORMATION;
}</jsScript_script>
