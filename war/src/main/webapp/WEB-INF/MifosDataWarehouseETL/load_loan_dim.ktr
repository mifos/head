  <step>
    <name>Extract Loan Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>
select a.account_id as loan_account_id,
a.global_account_num as loan_account_global_id,
lv.lookup_name as loan_status, la.loan_amount, 
ls.orig_principal as original_principal, ls.orig_interest as original_interest, ls.orig_fees as original_fees, ls.orig_penalty as original_penalty,
la.loan_amount_currency_id as currency_id, a.customer_id, la.prd_offering_id,
a.created_date

from account a
join loan_account la on la.account_id = a.account_id
join loan_summary ls on ls.account_id = a.account_id
join account_state ast on ast.account_state_id = a.account_state_id
join lookup_value lv on lv.lookup_id = ast.lookup_id 
order by a.customer_id</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
