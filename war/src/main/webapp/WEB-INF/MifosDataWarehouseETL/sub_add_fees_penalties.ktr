  <step>
    <name>Read account_trxn for Loan Fees</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select atxn.action_date, cast(ifnull(atxn.due_date, &apos;3001-01-01&apos;) as date) as due_date,
date(atxn.created_date) created_date, atxn.account_action_id,
atxn.account_trxn_id, ftxn.fee_trxn_detail_id, atxn.account_id as loan_account_id,  atxn.payment_id, atxn.installment_id, ap.payment_type_id,
ftxn.fee_amount as amount, 1 as account_charge_type_id, af.fee_id
from account_trxn atxn
inner join fee_trxn_detail ftxn on atxn.account_trxn_id = ftxn.account_trxn_id
inner join account_payment ap on ap.payment_id = atxn.payment_id
left join account_fees af on af.account_fee_id = ftxn.account_fee_id
where atxn.account_action_id in (1, 15, 20, 4)
and ftxn.fee_amount &lt;&gt; 0
and atxn.created_date &gt;= date(?) 
and atxn.created_date &lt; date(?)
&#47;* 
1  - &apos;AccountAction-LoanRepayment&apos;
15 - &apos;AccountAction-LoanWrittenOff&apos;
20 - &apos;AccountAction-LoanRescheduled&apos;
4 - &apos;AccountAction-Fee&apos;
*&#47;

</sql>
