  <step>
    <name>Identify Loan Account Status Changes</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>
select ah.changed_date as effective_date, -5 as level_id, a.account_id as entity_id, 
&apos;S&apos; as change_type, ah.account_status_change_id as change_order, 
lv.lookup_name as updated_status, 
null as updated_parent_id, null as  updated_loan_officer_id, null as updated_branch_id,
&apos;loan&apos; as account_type,
la.prd_offering_id as account_prd_offering_id,
la.loan_amount_currency_id as account_currency_id
from account a
join loan_account la on la.account_id = a.account_id
join account_status_change_history ah on ah.account_id = a.account_id
            and ah.account_status_change_id &gt; 
                    (select min(ahmin.account_status_change_id)
                        from account_status_change_history ahmin
                        where ahmin.account_id = a.account_id) 
			and (ah.changed_date between date(?) and date(?))

join account_state ast on ast.account_state_id = ah.new_status
join lookup_value lv on lv.lookup_id = ast.lookup_id

</sql>
    <limit>0</limit>
    <lookup>Get Start and End Date Variables</lookup>
    <execute_each_row>Y</execute_each_row>
    <variables_active>Y</variables_active>
