  <step>
    <name>Extract Personnel Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>
select p.personnel_id, p.display_name, lv.lookup_name gender, pd.government_id_number,

(case p.level_id
when 1 then 1
else 0
end) as is_loan_officer,

&#47;*1st time data changed to loan_officer.  However if personnel entry was created 
as a loan officer (and still is) then no change log would have been created... 
in this case the mfi joining date is used*&#47;
ifnull(cl.changed_date, 
    (case p.level_id
    when 1 then pd.date_of_joining_mfi
    else null
    end))  as date_became_loan_officer,

pd.date_of_joining_mfi, 

&#47;* if current status is inactive... then the date of becoming inactive is considered the date of leaving mfi *&#47;
if(ifnull(cldstatus.new_value, &apos;X&apos;) = &apos;Inactive&apos;, clstatus.changed_date, null) as date_of_leaving_mfi

from personnel p
join personnel_details pd on pd.personnel_id = p.personnel_id
left join lookup_value lv on lv.lookup_id = pd.gender

left join change_log cl on cl.change_log_id =  
    (select min(clmin.change_log_id) from change_log clmin
    join change_log_detail cldmin on cldmin.change_log_id = clmin.change_log_id 
                                            and cldmin.field_name = &apos;User Hierarchy&apos;
                                            and cldmin.new_value = &apos;Loan Officer&apos;
    where clmin.entity_id = p.personnel_id 
    and clmin.entity_type = 17) 

&#47;*look for max status*&#47;
left join change_log clstatus on clstatus.change_log_id = 
    (select max(clstatusmin.change_log_id) from change_log clstatusmin
    join change_log_detail cldstatusmin on cldstatusmin.change_log_id = clstatusmin.change_log_id 
                                            and cldstatusmin.field_name = &apos;Status&apos;
    where clstatusmin.entity_id = p.personnel_id 
    and clstatusmin.entity_type = 17) 
left join change_log_detail cldstatus on cldstatus.change_log_id = clstatus.change_log_id
                                            and cldstatus.field_name = &apos;Status&apos;

order by p.personnel_id

</sql>
