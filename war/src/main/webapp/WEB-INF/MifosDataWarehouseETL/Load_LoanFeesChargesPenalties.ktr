from account_trxn atxn
inner join  loan_trxn_detail ltxn on atxn.account_trxn_id = ltxn.account_trxn_id
inner join account_payment ap on ap.payment_id = atxn.payment_id
where atxn.account_action_id in (1, 15, 20)
and not (ltxn.misc_fee_amount = 0 and ltxn.misc_penalty_amount = 0)

and atxn.created_date &gt;= date(?) 
and atxn.created_date &lt; date(?)
