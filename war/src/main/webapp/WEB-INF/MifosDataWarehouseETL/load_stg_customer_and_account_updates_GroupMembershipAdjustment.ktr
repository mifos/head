    <connection>SourceDB</connection>
    <sql>select cl.changed_date as effective_date, c.customer_level_id as level_id, c.customer_id as entity_id, 
&apos;H&apos; as change_type, 999999999 as change_order, 
null as updated_status,  
ifnull(c.customer_formedby_id, 0) as customer_formedby_id,
cld.new_value
from customer c
join change_log cl on c.customer_id = cl.entity_id
                        and cl.entity_type = 1     
						and (cl.changed_date between date(?) and date(?))
						and cl.change_log_id = 
                            (select max(clmaxGF.change_log_id)
                            from change_log clmaxGF
                            join change_log_detail cldmaxGF on cldmaxGF.change_log_id = clmaxGF.change_log_id
                                                and cldmaxGF.field_name =  &apos;groupFlag&apos;
                            where clmaxGF.entity_id = c.customer_id
                            and clmaxGF.entity_type = 1
							and clmaxGF.changed_date = cl.changed_date)


                 
join change_log_detail cld on cld.change_log_id = cl.change_log_id
                        and cld.field_name =   &apos;groupFlag&apos;

where c.customer_level_id = 1

</sql>
