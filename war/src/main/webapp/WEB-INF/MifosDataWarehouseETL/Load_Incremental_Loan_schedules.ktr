    <connection>SourceDB</connection>
    <sql>select ls.id,
ls.account_id as loan_account_id, ls.installment_id,
ls.principal as principal_amount, 
ls.interest as interest_amount, 
ls.misc_fees as misc_fee_amount, 
(ls.misc_penalty+ls.penalty) as misc_penalty_amount,
ls.action_date as due_date
from loan_schedule ls
where last_updated &gt;= date(?) and last_updated &lt; date(?)</sql>
