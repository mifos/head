  <step>
    <name>Calculate days_in_arrears</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <rowlimit>0</rowlimit>
    <sql>select (to_days(?) - to_days(ls.due_date)) + 1 as days_in_arrears
from dw_loan_schedules ls
where ls.loan_account_id = ?
and ls.installment_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
