  <step>
    <name>Set DB Variables</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
if (Key == &quot;DestinationDB&#47;user&quot;)
{
	setVariable(&quot;db_username&quot;, Value, &quot;r&quot;);
	writeToLog(&quot;m&quot;, &quot;DEBUGGING: DestinationDB&#47;user: &quot; + Value);
}
if (Key == &quot;DestinationDB&#47;password&quot;)
{
	setVariable(&quot;db_password&quot;, Value, &quot;r&quot;);
	writeToLog(&quot;m&quot;, &quot;DEBUGGING: DestinationDB&#47;password: &quot; + Value);
}
if (Key == &quot;DestinationDB&#47;url&quot;)
{

	var cutoffFromQuestionMark;
	var questionPos = indexOf(Value,&quot;?&quot;);
	if (questionPos == -1)
	{
		cutoffFromQuestionMark = Value;
	}
	else
	{
		cutoffFromQuestionMark = substr(Value,0, questionPos);
	}

	var dbname;
	var hostname;
	var portnumber;
	var hostandport;
	var slashpos;
	var doubleslashpos;
	var colonpos;

	dbname = cutoffFromQuestionMark;
	slashpos = indexOf(cutoffFromQuestionMark,&quot;&#47;&quot;, 0);
	while ( slashpos != -1)
  	{
		dbname = substr(dbname, slashpos + 1);
		slashpos = indexOf(dbname,&quot;&#47;&quot;, slashpos) 
  	}



	doubleslashpos = indexOf(cutoffFromQuestionMark,&quot;&#47;&#47;&quot;, 0);
	hostandport = substr(cutoffFromQuestionMark, doubleslashpos + 2);
	slashpos = indexOf(hostandport,&quot;&#47;&quot;, 0);
	hostandport = substr(hostandport, 0, slashpos );

	colonpos = indexOf(hostandport,&quot;:&quot;, 0);
	hostname = substr(hostandport, 0, colonpos);
	portnumber = substr(hostandport, colonpos + 1);


	setVariable(&quot;dwdb&quot;, dbname, &quot;r&quot;);
	writeToLog(&quot;m&quot;, &quot;DEBUGGING: dwdb: &quot; + dbname);
	setVariable(&quot;hname&quot;, hostname, &quot;r&quot;);
	writeToLog(&quot;m&quot;, &quot;DEBUGGING: hname: &quot; + hostname);
	setVariable(&quot;portnum&quot;, portnumber, &quot;r&quot;);
	writeToLog(&quot;m&quot;, &quot;DEBUGGING: portnum: &quot; + portnumber);

}</jsScript_script>
      </jsScript>    </jsScripts>    <fields>    </fields>     <cluster_schema/>
