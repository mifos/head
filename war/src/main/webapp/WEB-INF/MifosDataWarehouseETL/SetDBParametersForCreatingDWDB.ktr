    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var etlDir = &quot;&quot;;
var fslash = &quot;\\\\&quot;;
var bslash = &quot;&#47;&quot;;

var ifnd = getVariable(&quot;Internal.Transformation.Filename.Directory&quot;, &quot;&quot;);

var osname = substr(getVariable(&quot;os.name&quot;, &quot;&quot;), 0, 7);
if (osname == &quot;Windows&quot;)
{
	etlDir = replace(ifnd,&quot;file:&#47;&#47;&#47;&quot;, &quot;&quot;);
	etlDir = replace(etlDir,bslash, fslash);
}
else
{
	etlDir = &quot;&#47;&quot; + replace(ifnd,&quot;file:&#47;&#47;&#47;&quot;, &quot;&quot;);
}

setVariable(&quot;etl_dir&quot;, etlDir, &quot;r&quot;);
setVariable(&quot;etl_job&quot;, &quot;Initial Incremental Entry&quot;, &quot;r&quot;);

writeToLog(&quot;m&quot;, &quot;etldir: &quot; + etlDir);
writeToLog(&quot;m&quot;, &quot;os.arch: &quot; + getVariable(&quot;os.arch&quot;, &quot;&quot;));
writeToLog(&quot;m&quot;, &quot;os.name: &quot; + getVariable(&quot;os.name&quot;, &quot;&quot;));
writeToLog(&quot;m&quot;, &quot;os.version: &quot; + getVariable(&quot;os.version&quot;, &quot;&quot;));


</jsScript_script>
