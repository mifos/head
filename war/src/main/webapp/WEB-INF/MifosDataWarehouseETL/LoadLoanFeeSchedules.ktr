    <sql>select c.customer_id, c.branch_id, 
c.loan_officer_id, c.customer_formedby_id, la.prd_offering_id,
ls.account_id, ls.installment_id,ls.principal_currency_id, 
ls.action_date as due_date, 
lfs.fee_id,
lfs.amount as fee_amount
from loan_fee_schedule lfs
inner join fees f on f.fee_id = lfs.fee_id
inner join loan_schedule ls on ls.id = lfs.id
inner join loan_account la on la.account_id = ls.account_id
inner join account a on a.account_id = la.account_id
inner join customer c on c.customer_id = a.customer_id</sql>
